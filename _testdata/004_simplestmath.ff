  &\text{b}\text{ } = \text{ }\text{2}\\
  &\text{b}\text{ } = \text{ }\text{(}\text{b}\text{ } + \text{ }\text{b}\text{)}\text{ } * \text{ }\text{b}\\
  &
}F{
  real(8) :: b
	b = 2
	b = (b + b) * b
}

